{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "barryvdh/laravel-debugbar": "^3.7",
        "fruitcake/laravel-cors": "dev-develop",
        "geo-sot/laravel-env-editor": "^3.1",
        "guzzlehttp/guzzle": "^7.2",
        "hansschouten/laravel-pagebuilder": "*",
        "inertiajs/inertia-laravel": "^2.0",
        "laravel/framework": "^10.10",
        "laravel/sanctum": "^3.2",
        "laravel/tinker": "^2.8",
        "league/commonmark": "^2.6",
        "league/html-to-markdown": "^5.1",
        "maatwebsite/excel": "^3.1",
        "predis/predis": "^2.3",
        "propaganistas/laravel-phone": "^5.3",
        "qirolab/laravel-themer": "^2.0",
        "qoraiche/laravel-mail-editor": "^4.1@beta",
        "rap2hpoutre/laravel-log-viewer": "^2.5",
        "rbdwllr/reallysimplejwt": "^5.0",
        "spatie/laravel-permission": "^6.16",
        "spatie/laravel-query-builder": "^6.3",
        "tightenco/ziggy": "^2.0",
        "yajra/laravel-datatables-oracle": "^10.11"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laravel/breeze": "^1.29",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.1",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Helpers/Constants.php",
            "app/Helpers/MailerHelper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
